{
  "extends": "//",
  "linter": {
    "rules": {
      "correctness": {
        "noUnknownProperty": "off"
      },
      "performance": {
        "noImgElement": "off"
      },
      "nursery": {
        "noAwaitInLoop": "off",
        "noUnknownAtRule": "off",
        "noNoninteractiveElementInteractions": "off"
      },
      "style": {
        "noHeadElement": "off"
      }
    }
  }
}
