{
  "extends": "//",
  "linter": {
    "rules": {
      "complexity": {
        "noExcessiveCognitiveComplexity": "warn"
      },
      "correctness": {
        "noUnknownProperty": "warn"
      },
      "performance": {
        "noImgElement": "off"
      },
      "nursery": {
        "noAwaitInLoop": "warn",
        "noUnknownAtRule": "warn",
        "noNoninteractiveElementInteractions": "off"
      },
      "style": {
        "noHeadElement": "off"
      }
    }
  }
}
